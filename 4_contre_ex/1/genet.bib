@techreport{bhk05:ir,
author = {Boichut, Y. and H\'eam, P.-C. and Kouchnarenko, O.},
title = {Automatic Verification of Security Protocols Using Approximations},
institution = {INRIA},
type = {Research Report},
number = {RR 5727},
ps = {/publis/pub/2005/RR2005-01.ps},
pdf = {/publis/pub/2005/RR2005-01.pdf},
year = 2005,
abstract = {Security protocols are widely used in open modern network to ensure safe communications. It is now widely recognized that formal analysis can provide the level of assurance required by both the developers and the users of the protocols. Unfortunately it is generally undecidable to certify whether a protocol is safe or not. However the automatic verification of security protocols can be attempted using abstraction-based approximation. Tree automata approximations for this purpose were introduced by Genet and Klay in 2000. We propose in this paper an extension of their techniques providing an efficient automatizing of the approach. We have implemented our contribution in the tool {    t TA4SP} accepting a high level specification language as input format and providing positive practical results on industrial security protocols.},
keyword = {Security protocols, Verification, Approximations, Abstractions},
note = {in revision for Journal of Automated Reasoning}

} 


