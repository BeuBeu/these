@TECHREPORT{kucherov92a,
	NUMBER = {92-R-062},
	AUTHOR = {G. Kucherov and M. Tajine},
	TITLE = {{Decidability of Regularity and Related Properties of Ground Normal Form Languages}},
	MONTH = jun,
	YEAR = {1992},
	ADDRESS = van,
	INSTITUTION = crin,
	TYPE = ri,
	NOTE = {Published in Proceedings 2nd International Workshop on Conditional
		   and Typed Rewriting Systems, Pont-{\`a}-Mousson (France)},
	ABSTRACT = 
{This paper introduces the property of finite irreducibility of a term
with respect to a variable and proves it to be decidable. It turns out
that his property generalizes numerous interesting properties of the
language of ground normal forms. In particular, we show that
regularity of the language can be expressed by this property. In this
way we prove the decidability of regularity of the set of ground
normal forms, the problem mentioned in the list of open problems in
rewriting. Also, the decidability of the existence of an equivalent
ground term rewriting system and some other results are proved.}
}

